  elseif  ( $SgMacro_number =  110  ) then
    evaluate ( $SgMacro_name = "I 41 C D" )
  elseif  ( $SgMacro_number =  111  ) then
    evaluate ( $SgMacro_name = "P -4 2 M" )
  elseif  ( $SgMacro_number =  112  ) then
    evaluate ( $SgMacro_name = "P -4 2 C" )
