skeletonize {<xray-skeletonize-statement>} end

<xray-skeletonize-statement> :==

  ?
  --{Print information about current status.}
  from=<real-space-object>
  --{Real space object to skeletonize.}
  minden=<real>
  --{Default=1.2}
  maxden=<real>
  --{Default=6.0}
  epden=<real>
  --{Default=2.0}
  peaken=<real>
  --{Default=2.0}
  b=<real>
  --{Default=5.0}
  maxgraphstats=<integer>
  --{Default=500}
  mingraph=<integer>
  --{Maximum is maxgraphstats. Default=15}
  maxnodefrac=<integer>
  --{Default=2}
  selection=<map-selection>
  --{Default=(all)}
  output=<filename>
  --{Default=output}
  pdbfile=<filename>
  --{Default=none}
  maxsearchdepth=<integer>
  --{Default=10000}
  smallelectrondensity=<real>
  --{Default=0.0005}
