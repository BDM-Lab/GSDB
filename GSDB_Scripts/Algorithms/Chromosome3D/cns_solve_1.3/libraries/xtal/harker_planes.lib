  eval(&harker.zx.1.y=0)
  eval(&harker.zx.2.note="m=2:x-z+3/4,1/4,x+z+1/4")
  eval(&harker.zx.2.y=0.25)
  eval(&harker.xy.n=2)
  eval(&harker.xy.xmin=0)
  eval(&harker.xy.xmax=0.5)
  eval(&harker.xy.ymin=0)
  eval(&harker.xy.ymax=0.5)
  eval(&harker.xy.1.note="m=2:2*x,2*y+1/2,0")
  eval(&harker.xy.1.z=0)
  eval(&harker.xy.2.note="m=2:x+y+1/4,-x+y+3/4,1/4")
  eval(&harker.xy.2.z=0.25)
else
  eval(&harker.found = false)
  display %HARKER-ERR: Harker planes for space group &sg do not exist in the library
  abort
end if

if ( &harker.found = true ) then
  display HARKER: found Harker planes for space group &sg in library
end if

set message=$message_old echo=$echo_old end

